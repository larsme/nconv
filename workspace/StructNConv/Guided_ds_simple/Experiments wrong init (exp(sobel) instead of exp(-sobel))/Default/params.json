{
    "exp_name": "exp_unguided_depth",
    "trainer": "KittiDepthTrainer",
    "use_gpu": true,
    "gpu_id": 0,
    "invert_depth": false,
    "data_normalize_factor": 256,
    "kitti_depth_dataset_dir": "../../data/kitti_depth",
    "kitti_rgb_dataset_dir": "../../data/kitti_raw",
    "load_rgb":true,
    "train_on": 10000,
    "train_batch_sz": 6,
    "val_batch_sz": 6,
    "test_batch_sz": 4,
    "loss": "ConfLossDecay",
    "optimizer": "Adam",
    "lr": 0.01,
    "weight_decay": 0,
    "lr_decay": 0.1,
    "lr_decay_step": 10,
    "enforce_pos_weights": "Softplus",
    "num_channels": 2,
    "init_method": "k",
    "maxpool_s": true,
    "nn_upsample_s": true,
    "use_conv_bias_s": false,
    "const_bias_init_s": false,
    "use_deconv_bias_s": false,
    "maxpool_d": true,
    "nn_upsample_d": true,
    "use_conv_bias_d": true,
    "const_bias_init_d": false,
    "use_deconv_bias_d": false,
    "devalue_pooled_confidence": true,
    "lidar_padding": 0,
    "num_epochs": 3,
    "save_chkpt_each": 1,
    "save_out_imgs": false,
    "notes_to_self": "Unguided depth completion network trained on Depth with smoothness"
}