    "mirror_weights": true,
    "num_channels": 1,
    "init_method": "nk",
    "maxpool_s": true,
    "nn_upsample_s": true,
    "maxpool_d": true,
    "nn_upsample_d": true,
    "devalue_pooled_confidence": true,
    "lidar_padding": 0,
    "crop_top": 0,
    "num_epochs": 7,
    "save_chkpt_each": 1,
