{
    "exp_name": "exp_unguided_depth",
    "trainer": "KittiDepthTrainer",
    "use_gpu": true,
    "gpu_id": 0,
    "invert_depth": false,
    "data_normalize_factor": 256,
	"kitti_depth_dataset_dir": "../../data/kitti_depth",
	"kitti_rgb_dataset_dir": "../../data/kitti_rgb",
    "train_on": 10000,
    "train_batch_sz": 8,
    "val_batch_sz": 10,
    "test_batch_sz": 10,
    "loss": "ConfLossDecay",
    "optimizer": "Adam",
    "lr": 0.01,
    "weight_decay": 0,
    "lr_decay": 0.1,
    "lr_decay_step": 10,
    "enforce_pos_weights": "Softplus",
    "lidar_padding": 0,
    "num_epochs": 6,
    "save_chkpt_each": 1,
    "save_out_imgs": false,
    "notes_to_self": "Unguided depth completion network trained on Depth"
}
